.login-box {
  width: 100%;
  min-height: 100vh;
  background-color: #fff
  background-size: 100% 100%;
  //background: url("../../assets/images/loginBg.png") no-repeat top;
  //background-size: 100% 50%;
}
.qidongye {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0
  top: 0
  z-index: 999999;
}
.loginBg {
  width: 100%;
}
.content-box {
  padding: 75px 20px 0 20px;
}
.content {
  width: 90%;
  background-color: #fff;
  box-shadow 0 0 10px #ccc;
  border-radius:10px;
  height 400px
  box-sizing border-box
  position: absolute;
  left: 50%;
  top: 240px;
  transform translateX(-50%)
}
.loginLogo {
  width: 100px;
  height: 100px;
  // background-color: #fff;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: 0px;
  transform translate(-50%,-50%);
  overflow hidden;
  text-align: center;
  display: flex;
  justify-content center
  align-items: center;
  
}
.loginLogo img {
  width: 90px;
  height: 90px;
  border-radius: 50%;
}
.logo {
  text-align: center;
  padding-top: 40px;
}
.login-img {
  height: 120px;
  object-fit: cover;
}
.phone-input {
  background-color: #F2F3F7;
  margin-top: 25px;
  text-align: center;
  margin-left: 30px;
  margin-right: 30px;
  height: 40px;
  line-height: 40px;
  border-radius: 10px;
  overflow: hidden;
  border:1px solid rgba(255,255,255,0.6);
  position: relative;
  padding-left: 30px;
  box-sizing border-box
}
.phone-input img {
  width: 20px
  height: 20px
  position: absolute;
  left:5px;
  top: 50%;
  transform translateY(-50%)
}
.phone-input input {
  width: 100%;
  height: 100%;
  //color: #FFFFFF;
  font-size: 16px;
  //text-align center
}
input::placeholder{
  //color:#ffffff !important;
  opacity 1
}
.get-code {
  margin: 20px 30px 0;
  overflow: hidden;
  border-radius: 20px !important;
}
.get-code-btn {
  width: 100%;
  height 40px;
  border-radius: 20px !important;
  background: #D33A26 !important;
  color: #FFFFFF !important;
  font-size: 16px;
}
.division {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 50px 17px 0 18px;
}
.hr {
  width: 100px;
  height: 1px;
  opacity: 0.3;
  background: #FFFFFF;
  /* border:1px solid rgba(255,255,255,.3); */
}
.division-text {
  font-size: 12px;
  color: #FFFFFF;
  opacity: 0.6;
  margin: 0 14px;
}
.icon-weixin {
  text-align: center;
  font-size: 40px;
  color: #FFFFFF;
  opacity: 0.6;
  margin-top: 40px;
  padding-bottom: 128px;
}
.active {
  background: #DF5959 !important;
}
.download {
  color #ffffff
  display flex
  align-items center
  justify-content center
}
.download-btn {
  margin-top 50px
  width 100px
  height 30px
  display flex
  align-items center
  justify-content center
  border 1px solid #ffffff
  font-size 14px
  border-radius 15px

}
.login-privacy{
  height: 25px;
  display: flex;
  align-items: center;
  margin-left: 0.8rem;
  margin-right: 0.8rem;
  margin-top: 15px;
  img{
    width: 15px;
    height: 15px;
  }
}
.login-treaty{
  margin-left: 5px;
  display: flex;  
}

.login-treaty-link{
  color: red;
  margin-right: 3px;  
}
>>> .van-dialog{
  background-color:rgba(255,255,255,.9)!important;
}
.dialog-box div{
  height:100%;
  line-height: 1.4;
}
.dialog h1{
   font-size: 16px;
    font-weight: 700;
    text-align: center;
      padding:8px 0;
}
.dialog-box p{
  padding:4px 8px;
}
.dialog-box h4{
  padding:4px 8px;
}
.register {
  width: 100%;
  margin: 10px auto 0;
  height: 30px;
  display: flex;
  justify-content space-between
  align-items: center;
  position: absolute;
  left: 0;
  bottom: 30px;
}
.registerBtn {
  width: 40%;
  //display: block;
  height: 30px;
  text-align: center;
  background-color: #fff;
  border-radius: 10px;
  //margin: 0 auto;
  color: #D59B3A;
  font-size: 12px
}
