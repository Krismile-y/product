.three {
  width: 90%;
  height: 300rpx;
  background-color: red;
  border-radius: 20rpx;
  margin: 0 auto;
  color: #fff;
  font-weight: 600;
}
.three .num {
  margin-top: 1rpx;
}
.three .one {
  width: 100%;
  height: 150rpx;
}
.three .two {
  width: 100%;
  height: 150rpx;
  display: flex;
}
.three .two .item {
  width: 50%;
  height: 100%;
}
.item {
  width: 90%;
  height: 110rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #cccccc;
  display: flex;
  align-items: center;
  position: relative;
}
.item .font {
  width: 90%;
  height: 100%;
  display: flex;
  align-items: center;
}
.item .btn {
  width: 100rpx;
  height: 50rpx;
  background-color: pink;
  color: #fff;
  border-radius: 5rpx;
}

