.example-body {
  display: flex;
  flex-direction: row;
}
.calendar-button {
  flex: 1;
  font-weight: bold;
  font-size: 32rpx;
}
.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 300rpx;
}
.bottom .in {
  width: 90%;
  height: 130rpx;
  margin-top: 30rpx;
  background-image: url(/static/common/btn.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  color: #fff;
  font-size: 40rpx;
  border-radius: 20rpx;
  margin: 0 auto;
}

