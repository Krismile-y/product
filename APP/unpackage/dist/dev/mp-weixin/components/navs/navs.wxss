.nav {
  background-color: #ffffff;
  width: 100%;
  height: 90rpx;
  position: relative;
}
.nav .icon {
  height: 100%;
  width: 120rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.nav .icon image {
  width: 80rpx;
  height: 80rpx;
}
.info {
  height: 100vh;
  width: 320rpx;
  position: relative;
}
.info .bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 320rpx;
  height: 200rpx;
  font-size: 28rpx;
  color: #cccccc;
}
.info .bottom view {
  margin-bottom: 10rpx;
}
.info .bottom .btn {
  width: 140rpx;
  height: 70rpx;
  color: #fff;
  background-color: greenyellow;
  border-radius: 15rpx;
  margin-top: 10rpx;
}
.info .title {
  width: 100%;
  height: 90rpx;
  font-size: 36rpx;
  border-bottom: 1rpx solid #ccc;
  display: flex;
  align-items: center;
  justify-content: center;
}
.info .item {
  width: 100%;
  height: 110rpx;
  display: flex;
  align-items: center;
}
.info .item image {
  width: 45rpx;
  height: 45rpx;
  margin-left: 50rpx;
  margin-right: 20rpx;
}
.info .item .titles {
  padding-top: 0rpx;
}

